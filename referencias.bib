	file = {Miosso et al. - 2009 - Compressive sensing method for improved reconstruc.pdf:/home/gabrielziegler/Zotero/storage/NL2GPNS7/Miosso et al. - 2009 - Compressive sensing method for improved reconstruc.pdf:application/pdf}
}

